gsmatch=lequal,1,0
objlib /inc=idlsql_syb
symbol_vector=(IDLSQL_DYNAMIC=procedure,-
	       IDLSQL_INTERACTIVE=procedure, -
	       IDLSQL_LOGIN=procedure, -
	       IDLSQL_LOGOUT=procedure, -
	       IDLSQL_SETDBTYPE=procedure -
)
objlib /lib
sys$share:idltransfer.exe /share
sys$share:dblib.exe /share
