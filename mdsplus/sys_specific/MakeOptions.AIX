CFLAGS = -I../include -g -Dunix -D_big_endian -qchars=signed
MOTIF_CFLAGS = -I../include -g -D_NO_DXm
WMD_RULE = ../bin/wmdcreate ; ../bin/wmdcreate -o ../shlib/xmdsuil.wmd
WML = touch UilConst.h 
SHARE_TYPE = a
MAKE_DEPEND = cc $(CFLAGS) -M -E {} > /dev/null \; ; rm -f .depend ; cat *.u >> .depend ; rm -f *.u
LD_SHARED_LIB_OPTIONS = -bnoentry -bM:SRE -bE:$(EXPORTS)
DLD = -L../shlib -ldl
MOTIF_LIBS = -lMrm -lXm
X11_LIBS = -lXt -lX11
