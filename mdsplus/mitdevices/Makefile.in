EXPORTS = MitDevices.exports


all :  @MAKELIBDIR@ @MAKESHLIBDIR@ @MAKEBINDIR@ \
       @MAKESHLIBDIR@libMitDevices@SHARETYPE@ @MAKELIBDIR@libMitDevices.a \
       @MAKEBINDIR@daq_server  $(UIDS) 



depend:
	@ :

clean:
	@ $(RM) $(UIDS)
	@ $(RM) $(OBJECTS)
	@ $(RM) so_locations
	@ $(RM) @MAKESHLIBDIR@libio@SHARETYPE@
	@ $(RM) @MAKELIBDIR@libio.a

install:
	for uid in $(UIDS) ; do\
	  $(INSTALL) -m 644 $${uid} @uiddir@;\
	done
	if [ ! -d @libdir@ ] ; then \
	        mkdir @libdir@; \
	        chmod 755 @libdir@; \
	fi;
	$(INSTALL) -m 755  @MAKESHLIBDIR@libio@SHARETYPE@ @libdir@
	if (test @SHARETYPE@ != .a) then \
	  $(INSTALL) -m 644  @MAKELIBDIR@libio.a @libdir@; \
	fi;

@MAKEBINDIR@ :
	mkdir @MAKEBINDIR@

@MAKEBINDIR@daq_server : daq_server
	cp daq_server @MAKEBINDIR@daq_server

@MAKESHLIBDIR@libMitDevices@SHARETYPE@ : $(OBJECTS) @EXPORTS_FILE@
	$(LD) -o $@ @LINKSHARED@ $(OBJECTS) $(LIBS)

@MAKELIBDIR@libMitDevices.a : $(OBJECTS)
	$(AR) -cr $@ $(OBJECTS)
	@RANLIB@ $@

@MAKELIBDIR@ :
	mkdir @MAKELIBDIR@

MitDevices.exports: MitDevices.def
	sed -e /LIBRARY/\;/\;/d io.def > $@

