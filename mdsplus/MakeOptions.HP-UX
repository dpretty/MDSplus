CFLAGS = +z -Ae -I../include -g 
MOTIF_CFLAGS = +z -Ae -I../include -I/usr/include/X11R6 -g -D_NO_DXm
SHARE_TYPE = sl
MAKE_DEPEND = cc $(CFLAGS) -P -Wp,-M {} 2> .depend \; ; rm *.i
LD_SHARED_LIB_OPTIONS = -b -ldld
CC_IMAGE_OPTIONS = -Wl,+s
MOTIF_LIBS = -L/usr/lib/X11R6 -L/usr/lib/Motif1.2 -lMrm -lXm
X11_LIBS = -lXt -lX11
